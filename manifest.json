{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Easy Note Extension",
    "version": "1.0",
    "action": {
      "default_popup": "/popup/popup.html",
      "default_icon": "/images/hello_extensions.png"
    },
    "permissions": [
      "tabs",
      "storage"
    ]
  }