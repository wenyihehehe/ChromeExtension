{
    "manifest_version": 3,
    "name": "My Note",
    "description": "Simple Note Extension",
    "version": "1.0",
    "action": {
      "default_popup": "/popup/popup.html",
      "default_icon": "/images/favicon16.png"
    },
    "permissions": [
      "tabs",
      "storage"
    ],
    "icons": {
      "16": "/images/favicon16.png",
      "32": "/images/favicon32.png",
      "48": "/images/favicon128.png",
      "128": "/images/favicon128.png"
    }
  }